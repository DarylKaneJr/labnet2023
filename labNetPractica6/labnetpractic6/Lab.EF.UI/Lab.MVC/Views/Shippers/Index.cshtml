@model List<Lab.MVC.Models.ShippersView>
@{
    ViewBag.Title = "Shippers ABMC";
}
<hr />
<h1>Tabla Shippers</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">ShipperID</th>
            <th scope="col">CompanyName</th>
            <th scope="col">Phone</th>
            <th scope="col">Action</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.ShippersID</th>
                <td>@item.CompanyName</td>
                <td>@item.Phone</td>
                <td>@Html.ActionLink("Borrar", "Delete", new { id = item.ShippersID }, new { onclick = "return confirm('Esta seguro de borrar ?')" })</td>
                <td>@Html.ActionLink("Editar", "Update", new { id = item.ShippersID })</td>
            </tr>
        }
    </tbody>
</table>

<div>
    @Html.ActionLink("Add new shipper", "Insert");
</div>



